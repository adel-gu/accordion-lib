{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "build-storybook": {
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "storybook": {
      "cache": false
    },
    "lint": {
      "cache": true
    },
    "format": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "defaultBase": "master"
}
